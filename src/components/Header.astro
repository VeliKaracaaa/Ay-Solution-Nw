<!-- Height h-37 because we want to heade to be sticky later -->
<header class="flex justify-between items-center h-37 py-1 px-20 bg-white">
    <!-- Autre dimension logo h-48 w-52 -->
    <Image src={aySolutionLogo} alt="ay solution logo" class="h-37 w-40" />
    <Navigation />
</header>

